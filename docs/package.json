{
  "name": "@versioneone/ui-docs",
  "version": "0.0.1-alpha",
  "description": "Documentation site for VersionOne UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/versionone/vue.git"
  },
  "engines": {
    "node": ">=4"
  },
  "private": true,
  "scripts": {
    "start": "npm run browser:development",
    "browser:development": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --inline",
    "browser:build": "cross-env NODE_ENV=production webpack --config webpack-production.config.js --progress --colors --profile",
    "browser:prd": "cross-env NODE_ENV=production webpack-dev-server --config webpack-production.config.js --progress --colors",
    "gh-pages:build": "node scripts/gh-pages-build.js"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.9.1",
    "babel-standalone": "^6.14.0",
    "camelcase": "^3.0.0",
    "codemirror": "^5.18.2",
    "front-matter": "^2.1.0",
    "lunr": "^0.7.1",
    "material-ui": "^0.15.4",
    "rc-collapse": "^1.6.6",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "react-title-component": "^1.0.1",
    "simple-assign": "^0.1.0",
    "underscore": "^1.8.3",
    "uniqueid": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.23.0",
    "doctrine": "^1.1.0",
    "eslint": "^3.0.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^5.0.1",
    "highlight.js": "^9.5.0",
    "history": "^2.0.0",
    "intl": "^1.2.3",
    "intl-locales-supported": "^1.0.0",
    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "raw-loader": "^0.5.1",
    "react-addons-perf": "^15.4.0",
    "react-docgen": "^2.9.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-motion": "^0.4.2",
    "react-router": "^2.5.2",
    "react-swipeable-views": "^0.6.3",
    "recast": "^0.11.10",
    "style-loader": "0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.10.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      [
        "transform-replace-object-assign",
        "simple-assign",
        "react-hot-loader/babel"
      ]
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          [
            "transform-replace-object-assign",
            "simple-assign"
          ]
        ]
      }
    }
  }
}
