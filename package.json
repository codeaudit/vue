    "react-addons-test-utils": "15.0.0-rc.2",
    "react-dom": "15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-storybook-addon-add-stories-group": "^0.1.0",
    "simulant": "^0.2.2",
    "style-loader": "^0.13.1",
