    "@kadira/storybook-deployer": "^1.0.0",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "7.1.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
