{
  "name": "versionone-ui",
  "version": "1.0.0",
  "description": "Common UI components used by VersionOne.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "build": "node_modules/.bin/babel src --out-dir ./",
    "test": "cross-env node_modules/.bin/mocha --require specSetup.js --compilers js:babel-core/register src/**/*.spec.js --recursive"
  },
  "keywords": [
    "VersionOne",
    "react",
    "components",
    "UI"
  ],
  "author": "Andrew Smith <andrew@WeAreSoftwareCraftsmen.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3",
      "stage-2",
      "stage-1"
    ]
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "enzyme": "^2.3.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "phantomjs-polyfill": "0.0.2",
    "react-addons-test-utils": "^15.0.0-rc.2",
    "rimraf": "^2.5.2",
    "sinon": "^2.0.0-pre"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
