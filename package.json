    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.10.2",
    "eslint-config-andrew-codes": "^3.1.1",
    "eslint-config-andrew-codes-react": "^1.1.2",
    "eslint-friendly-formatter": "^3.0.0",
