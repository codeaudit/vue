{
  "name": "@versionone/ui",
  "version": "3.0.3",
  "description": "Open-source and community supported collection of common UI components built with React. As an open-sourced and community supported project, VersionOne UI is not formally supported by VersionOne.",
  "engines": {
    "node": "^6.10.0",
    "yarn": "^0.23.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/versionone/vue.git"
  },
  "scripts": {
    "bootstrap": "build/bin/bootstrap.sh",
    "bootstrap/single": "build/bin/bootstrap.single.sh",
    "bootstrap/single/with-deps": "build/bin/bootstrap-with-deps.single.sh",
    "lint": "build/bin/lint.sh",
    "prepublishOnly": "echo 'use `gulp publish ${version type}` instead.' exit 1",
    "prepublish/icons": "build/bin/prepublish-icons.sh",
    "prepublish/single": "cross-env NODE_ENV=production ./build/bin/prepublish.single.sh",
    "start": "build/bin/start-storybook.sh",
    "test": "cross-env NODE_ENV=test jest --config .jestrc.json",
    "verify": "node build/scripts/verify.js"
  },
  "keywords": [
    "react",
    "components",
    "VersionOne",
    "UI"
  ],
  "author": "Andrew Smith <andrew@andrew.codes>",
  "license": "MIT",
