{
  "name": "@versionone/ui-theme-provider",
  "version": "0.0.0",
  "ui/name":"ThemeProvider",
  "ui/status": "experimental",
  "description": "Enable application of a common theme across all VersionOne UI components, as well as override specific component tree instances with different themes.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "ui/webpack/entry": "src/index.js",
  "ui/webpack/output": "dist/index.js",
  "scripts": {
    "prepublish": "{ cd ../../; yarn run prepublish/single @versionone/ui-theme-provider; };"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/versionone/vue.git"
  },
  "config": {
    "access": "public"
  },
  "keywords": [
    "theme",
    "styles",
    "css",
    "stylesheet",
    "less",
    "sass",
    "scss",
    "inline"
  ],
  "author": "VersionOne",
  "maintainers": [
    "Andrew Smith <andrew@andrew.codes>"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "15.1.0"
  },
  "dependencies": {
    "@versionone/ui-themes": "0.0.0"
  }
}
